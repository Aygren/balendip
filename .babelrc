{
    "presets": [
        [
            "next/babel",
            {
                "preset-react": {
                    "runtime": "automatic"
                }
            }
        ]
    ],
    "plugins": [
        [
            "babel-plugin-transform-imports",
            {
                "@supabase/supabase-js": {
                    "transform": "@supabase/supabase-js/${member}",
                    "preventFullImport": true
                }
            }
        ]
    ],
    "env": {
        "test": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets": {
                            "node": "current"
                        }
                    }
                ],
                "@babel/preset-react",
                "@babel/preset-typescript"
            ]
        }
    }
}