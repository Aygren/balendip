{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/sw.js",
            "headers": {
                "cache-control": "s-maxage=0"
            }
        },
        {
            "src": "/manifest.json",
            "headers": {
                "cache-control": "s-maxage=0"
            }
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ],
    "env": {
        "NEXT_PUBLIC_SUPABASE_URL": "@supabase_url",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@supabase_anon_key"
    }
}